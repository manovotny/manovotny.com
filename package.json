{
  "name": "manovotny.com",
  "description": "The Jekyll site for manovotny.com.",
  "version": "1.0.0",
  "author": {
    "name": "Michael Novotny",
    "email": "manovotny@gmail.com",
    "url": "https://manovotny.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manovotny/manovotny.com.git"
  },
  "engines": {
    "node": ">=8.9.3",
    "yarn": ">=1.3.2"
  },
  "scripts": {
    "dev": "node server.js",
    "eslint": "eslint . --ext .json --ext .js",
    "build": "rm -rf .next-export && next build && next export -o .next-export && mv -v .next-export/static/* .next-export/ && rm -rf .next-export/static",
    "lint": "yarn stylelint && yarn eslint",
    "start": "NODE_ENV=production node server.js",
    "stylelint": "stylelint \"**/*.js\"",
    "test": "yarn lint"
  },
  "dependencies": {
    "@zeit/next-mdxc": "0.0.3",
    "next": "5.1.0",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "styled-reset": "1.3.2"
  },
  "devDependencies": {
    "babel-plugin-inline-react-svg": "0.5.2",
    "babel-plugin-styled-components": "1.5.1",
    "eslint": "4.19.1",
    "eslint-config-get-off-my-lawn": "4.0.0",
    "globby": "8.0.1",
    "styled-components": "3.2.5",
    "stylelint-config-get-off-my-lawn": "2.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.3.1"
  }
}
