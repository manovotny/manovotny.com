{
    "name": "manovotny.com",
    "description": "My personal website.",
    "version": "0.0.0",
    "author": {
        "name": "Michael Novotny",
        "email": "manovotny@gmail.com",
        "url": "https://manovotny.com"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/manovotny/manovotny.com.git"
    },
    "engines": {
        "node": "8.11.3",
        "yarn": ">=1.0.0"
    },
    "files": [
        "components",
        "data",
        "icons",
        "pages",
        "static",
        "styles",
        "utils"
    ],
    "scripts": {
        "dev": "next",
        "deploy": "yarn export && cd out && now",
        "export": "node scripts/export.js",
        "lint": "eslint . --ext .json --ext .js",
        "precommit": "yarn pretty-quick",
        "prettier": "prettier --write \"**/*.{js,json,md,mdx}\"",
        "pretty-quick": "pretty-quick --staged",
        "release": "cd out && now alias",
        "start": "yarn export && serve out",
        "test": "yarn lint"
    },
    "dependencies": {
        "@mdx-js/mdx": "0.12.0",
        "@mdx-js/tag": "0.11.0",
        "luxon": "1.3.1",
        "next": "6.1.1",
        "next-ga": "1.0.3",
        "react": "16.4.1",
        "styled-components": "3.3.3",
        "styled-reset": "1.3.5",
        "title": "3.3.0"
    },
    "devDependencies": {
        "@zeit/next-mdx": "1.1.0",
        "babel-plugin-inline-react-svg": "0.5.3",
        "babel-plugin-styled-components": "1.5.1",
        "eslint": "4.19.1",
        "eslint-config-get-off-my-lawn": "4.0.0",
        "eslint-config-prettier": "2.9.0",
        "execa": "0.10.0",
        "globby": "8.0.1",
        "husky": "0.14.3",
        "listr": "0.14.1",
        "prettier": "1.13.7",
        "prettify-xml": "1.2.0",
        "pretty-quick": "1.6.0",
        "react-dom": "16.4.1",
        "serve": "9.1.2",
        "shelljs": "0.8.2"
    }
}
